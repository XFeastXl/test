Window:Restore()
end)

Window.Minimized:Connect(function(isMinimized)
minimizeButton.Visible = not isMinimized
restoreButton.Visible = isMinimized
end)
        else
            game.Players.LocalPlayer:Kick("You Are Not Whitelisted, Make a ticket")
        end
    else
        game.Players.LocalPlayer:Kick("You Are Not Whitelisted, Make a ticket")
    end
end

verifyUser ()
end
